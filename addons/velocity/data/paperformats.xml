<odoo>
    <data>
         <record id="a4_zero_paperformat" model="report.paperformat">
            <field name="name">A4 Header Zero</field>
            <field name="format">A4</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">10</field>
            <field name="margin_right">0</field>
            <field name="margin_left">0</field>
            <field name="orientation">Portrait</field>
            <field name="dpi">90</field>
            <field name="header_spacing">200</field>
            <field name="header_line" eval="False"/>
        </record>

         <record id="a4_custom_paperformat" model="report.paperformat">
            <field name="name">A4 Custom</field>
            <field name="format">A4</field>
            <field name="margin_top">70</field>
            <field name="margin_bottom">10</field>
            <field name="margin_right">2</field>
            <field name="margin_left">2</field>
            <field name="orientation">Portrait</field>
            <field name="dpi">90</field>
            <field name="header_spacing">60</field>
            <field name="header_line" eval="False"/>
        </record>
    </data>
</odoo>
